<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="m">
	
	<select id="select_id" resultType="member" parameterType="String">
		select * from member where id=#{id}
	</select>

</mapper>












